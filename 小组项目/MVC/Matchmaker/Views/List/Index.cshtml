                $("#nv").text("女性信息列表");
                $("#xingbie1").css("display", "none");
                $("#nanxing").css("display", "none");
            }
            var url = "http://localhost:50458"
            $("#Button1").click(function () {
                $.ajax({
                    url: url + "/api/Default",
                    type: "get",
                    success: function (data2) {
                        $("#ListTb tr:gt(0)").remove();
                        $("#dataList").tmpl(data2).appendTo("#ListTb");
                    }
                })
            })
            $("#Button2").click(function () {
