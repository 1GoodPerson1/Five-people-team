    <script type="text/javascript">
        $(function () {
            var nan = $("#nan").text();
            var nv = $("#nv").text();
            if (nan == "1") {
                $("#nan").text("男性信息列表");
                $("#nv").text("女性信息列表");
                $("#xingbie").css("display", "none");
                $("#nvxing").css("display", "none");
            } else if (nv == "0") {
